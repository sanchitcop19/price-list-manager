{
  "name": "res-pricelist",
  "version": "1.0.0",
  "description": "Price List Management for Roseate Engineering Services",
  "homepage": "./",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "8204",
    "category": "app.category.type",
    "mac": {
      "target": "dmg",
      "icon": "build/icon.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": "Sanchit Batra",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "mongodb": "^3.3.4"
  },
  "devDependencies": {}
}
